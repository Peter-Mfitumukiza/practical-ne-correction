{
    "swagger": "2.0",
    "info": {
        "title": "Vehicle Management System APIs",
        "version": "1.0.0",
        "description": "API documentation for Vehicle Management System"
    },
    "tags": [
        {
            "name": "Auth",
            "description": "Operations related to authentication"
        },
        {
            "name": "Vehicle",
            "description": "Operations related to vehicles"
        },
        {
            "name": "Vehicle Owner",
            "description": "Operations related to vehicle owner"
        }
    ],
    "servers": [
        {
            "url": "http://localhost:3001"
        }
    ],
    "paths": {
        "/api/v1/auth/register": {
            "post": {
                "summary": "Save user",
                "tags": [
                    "Auth"
                ],
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "in": "body",
                        "name": "body",
                        "properties": {
                            "fullname": {
                                "type": "string",
                                "example": "Mfitumukiza Peter"
                            },
                            "email": {
                                "type": "string",
                                "example": "ychag@example.com"
                            },
                            "phoneNumber": {
                                "type": "string",
                                "example": "0987654321"
                            },
                            "nationalId": {
                                "type": "string",
                                "example": "1234567890"
                            },
                            "password": {
                                "type": "string",
                                "example": "random"
                            }
                        }
                    }
                ],
                "responses": {
                    "201": {
                        "description": "User saved successfully"
                    },
                    "400": {
                        "description": "Bad request"
                    }
                }
            }
        },
        "/api/v1/auth/login": {
            "post": {
                "summary": "Login user",
                "tags": [
                    "Auth"
                ],
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "in":"body",
                        "name":"body",
                        "properties":{
                            "email":{
                                "type":"string",
                                "example": "ychag@example.com"
                            },
                            "password":{
                                "type":"string",
                                "example": "random"
                            }
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Logged in successfully"
                    },
                    "400": {
                        "description": "Bad request"
                    }
                }
            }
        }
    }
}